    <Technology id="1072" key="e9bcc246-4749-4d9c-bb5f-40dcf42f4088" parentID="1049" level="2" creatorID="0" sortOrder="1" createDate="2015-10-03T21:42:18" updateDate="2015-10-04T09:52:54" nodeName="Technology" urlName="technology" path="-1,1049,1072" isDoc="" nodeType="1068" creatorName="Dave Gordon" writerName="Dave Gordon" writerID="0" template="1069" nodeTypeAlias="Technology">
